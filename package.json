{
  "name": "ember-dyna-table",
  "version": "0.0.3",
  "description": "Extenible table component for dynamic tables.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "chilicoder1@gmail.com",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/ember-dyna-table/ember-dyna-table",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "coverage": "COVERAGE=true ember test;npm run replace",
    "replace": "node ./node_modules/replace-in-file/bin/cli.js '/SF:ember-dyna-table/g' 'SF:addon' ./coverage/lcov.info --isRegex",
    "docs": "ember build -o docs/ -e docs",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "codacy-coverage": "^2.1.0",
    "ember-cli-babel": "^6.12.0",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-truth-helpers": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "bootstrap": "^4.0.0",
    "broccoli-asset-rev": "^2.6.0",
    "broccoli-funnel": "^2.0.1",
    "ember-ajax": "^3.0.0",
    "ember-array-helper": "^3.0.0",
    "ember-cli": "~3.1.2",
    "ember-cli-addon-docs": "^0.4.1",
    "ember-cli-addon-docs-yuidoc": "^0.2.0",
    "ember-cli-code-coverage": "^0.4.2",
    "ember-cli-dependency-checker": "^2.1.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.1",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.7.0",
    "ember-cli-page-object": "^1.15.0-beta.1",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^2.0.2",
    "ember-code-example-component": "^0.2.0",
    "ember-code-snippet": "^2.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.5.0",
    "ember-source": "~3.1.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint-plugin-ember": "^5.0.3",
    "eslint-plugin-node": "^6.0.1",
    "loader.js": "^4.6.0",
    "replace-in-file": "^3.1.1"
  },
  "engines": {
    "node": ">= 6"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">2.8.0"
    }
  },
  "homepage": "https://ember-dyna-table.github.io/ember-dyna-table"
}
